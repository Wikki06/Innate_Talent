month=int(input())
if(month==1):
    print("Sunday")
elif(month==2):
    print("Monday")
elif(month==3):
    print("Tuesday")
elif(month==4):
    print("Wednesday")
elif(month==5):
    print("Thursday")
elif(month==6):
    print("Friday")
elif(month==7):
    print("Saturday")
else:
    print("Invalid day")
