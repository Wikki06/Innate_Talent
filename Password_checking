string=input()
upper=0
lower=0
special=0
digit=0
length=len(string)
for i in string:
    if(i.isupper()):
        upper=1
    elif(i.islower()):
        lower=1
    elif(i.isdigit()):
        digit=1
    else:
        special=1
if(upper+lower+digit+special==4):
    print("Strong password")
else:
    print("Weak password")
